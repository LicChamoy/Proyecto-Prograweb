<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Publicacion</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.Publicacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilUsuarioServlet</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.PerfilUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaServlet</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.BusquedaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarPorCategoria</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.BuscarPorCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarPorFecha</servlet-name>
        <servlet-class>com.mycompany.proyectoweb.resources.BuscarPorFecha</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/InicioSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/Dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Publicacion</servlet-name>
        <url-pattern>/Publicacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilUsuarioServlet</servlet-name>
        <url-pattern>/PerfilUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaServlet</servlet-name>
        <url-pattern>/busquedaservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarPorCategoria</servlet-name>
        <url-pattern>/BuscarPorCategoria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarPorFecha</servlet-name>
        <url-pattern>/BuscarPorFecha</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            300
        </session-timeout>
    </session-config>
</web-app>
